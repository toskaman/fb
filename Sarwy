_G.SelectRes=""_G.AutoRess=false;_G.RFound=false;_G.WalkTOThing=false;print("UI Library by ShowerHeadFD")local a=loadstring(game:HttpGet("https://raw.githubusercontent.com/toskaman/fb/master/SH_Lib"))()local b=a:CreateWindow("G")local c=b:Section("Menu")c:AddLabel("Select ressource to farm")local d={}for e,f in pairs(game:GetService("Workspace").Resources:GetChildren())do if not table.find(d,f.Name)then table.insert(d,f.Name)end end;c:AddBox("Example: Snowy tree",function(g)for e,f in pairs(game:GetService("Workspace").Resources:GetChildren())do if string.find(f.Name:lower(),tostring(g))then _G.SelectRes=tostring(f.Name)local h=f:FindFirstChildWhichIsA("Part")if h then local i=Instance.new("BillboardGui")local j=Instance.new("TextLabel")i.Parent=h;i.AlwaysOnTop=true;i.LightInfluence=1;i.Size=UDim2.new(0,20,0,20)i.StudsOffset=Vector3.new(0,0,0)i.Name="GigoBigoulah"j.Parent=i;j.BackgroundColor3=Color3.new(1,1,1)j.BackgroundTransparency=1;j.Size=UDim2.new(2,0,2,0)j.Text=f.Name;j.TextColor3=Color3.new(255,0,0)j.TextScaled=true;game.StarterGui:SetCore("SendNotification",{Title="Ressource",Text="Ressource found : "..tostring(f.Name),Duration=1})_G.RFound=true end end end;if not _G.RFound then game.StarterGui:SetCore("SendNotification",{Title="Ressource",Text="Ressource NOT FOUND !",Duration=1})_G.RFound=false end end)c:AddDrop("Or select (no ESP) :",d,function(k)_G.SelectRes=tostring(k)end)c:AddToggle("Farm Ressources",function(l)if l==true then _G.AutoRess=true else _G.AutoRess=false end end)c:AddToggle("Walk to Ressources",function(m)if m==true then _G.WalkTOThing=true else _G.WalkTOThing=false end end)c:AddButton("Clear ESP",function()pcall(function()for e,f in pairs(game:GetService("Workspace").Resources:GetDescendants())do if f:IsA("BillboardGui")then if tostring(f.Name)=="GigoBigoulah"then f:Destroy()end end end end)end)local n=b:Section("Settings")n:AddLabel("By MSDOS#8293")n:AddButton("Reconnect",function()pcall(function()game:GetService("TeleportService"):Teleport(game.PlaceId,game:GetService("Players").LocalPlayer)end)end)n:AddButton("Destroy GUI",function()a:Destroy()end)n:AddBind("Close Keybind",Enum.KeyCode.G,function()a:OpenClose()end)function getNearestTrain()nearest,dist=nil,99999;for o,p in pairs(game:GetService("Workspace").Resources:GetChildren())do if tostring(p.Name)==tostring(_G.SelectRes)then if p:FindFirstChild("Health")then if p.Health.Value>0 then local q=p:FindFirstChildWhichIsA("Part")if q then local r=game:GetService("Players").LocalPlayer;local s=r.Character;local t=s.HumanoidRootPart;local u=(t.Position-q.Position).magnitude;if u<dist then dist=u;nearest=q end end end end end end end;spawn(function()while wait()do if _G.AutoRess then getNearestTrain()if nearest~=nil then if(nearest.Position-game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude<10 then local v={[1]=nearest}local w=game:GetService("ReplicatedStorage").Events.SwingTool;w:FireServer(v)else if _G.WalkTOThing then game.Players.LocalPlayer.Character.Humanoid.WalkSpeed=90;game.Players.LocalPlayer.Character.Humanoid:MoveTo(nearest.Position)end end end end end end)
