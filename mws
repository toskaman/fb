function getNearestChest()nearest,dist=nil,99999;for a,b in pairs(game:GetService("Workspace").TargetStorage:GetDescendants())do local c=b:FindFirstChildWhichIsA("MeshPart")if c then local d=c:FindFirstChildWhichIsA("ClickDetector")if d then local e=d:FindFirstChild("Current_HP")if e then if tonumber(e.Value)>0 then if c.Parent:IsA("Folder")then local f=game:GetService("Players").LocalPlayer;local g=f.Character;local h=g.HumanoidRootPart;local i=(h.Position-c.Position).magnitude;if i<dist then dist=i;nearest=b end end end end end end end end;while wait()and _G.Auto_Farm do getNearestChest()local c=nearest:FindFirstChildWhichIsA("MeshPart")local d=c:FindFirstChildWhichIsA("ClickDetector")local e=d:FindFirstChild("Current_HP")game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame=c.CFrame;wait()fireclickdetector(d)wait()pcall(function()repeat wait()firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart,game:GetService("Workspace").Map["Sell_Part"],0)firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart,game:GetService("Workspace").Map["Sell_Part"],1)local e=d:FindFirstChild("Current_HP")until tonumber(e.Value)<=0 or e.Value==nil or _G.Auto_Farm==false end)local j={[1]="Rank_Upgrade_Request"}local k=game:GetService("ReplicatedStorage").RemoteEvent;k:FireServer(j)local j={[1]="Capacity_Upgrade_Request"}local k=game:GetService("ReplicatedStorage").RemoteEvent;k:FireServer(j)for l=1,10 do local j={[1]="UnlockMapStage_Request",[2]=l}local k=game:GetService("ReplicatedStorage").RemoteEvent;k:FireServer(j)end;local m=game:GetService("ReplicatedStorage")local n=m:WaitForChild("RemoteEvent")n:FireServer({"Rebirth_Request"})end
